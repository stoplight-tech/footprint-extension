{
  "name": "Chrome Extension Boilerplate",
  "description": "Chrome Extension boilerplate created with React Typescript.",
  "manifest_version": 3,
  "version": "1.0.0",
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["webRequest", "storage", "cookies"],
  "host_permissions": [
    "*://*/*"
  ]
}